<%if $admin%>
<%= form_for MainCategoryPic.new, :url => {:controller => 'main_category_pics', :action => 'create',  :method => 'post'} do |f| %>
       <div class="form-group">
         <%= f.label :pic_url %>
         <%= f.file_field :pic_url %>
         <%= f.hidden_field :pic_url_cache %>
       </div>
        <%= f.submit "Update", class: 'btn btn-success' %>
<%end%>
<%end%>
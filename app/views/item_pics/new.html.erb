<%= form_for ItemPic.new, :url => {:controller => 'item_pics', :action => 'create',  :method => 'post'} do |f| %>
       <div class="form-group">
         <%= f.label :pic_url %>
         <%= f.file_field :pic_url %>
         <%= f.hidden_field :pic_url_cache %>
       </div>
        <%= f.submit "Update", class: 'btn btn-success' %>
<%end%>
